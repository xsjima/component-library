{
  "name": "component-library",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xsjima/component-library.git"
  },
  "author": "Zabavnikov Denis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xsjima/component-library/issues"
  },
  "homepage": "https://github.com/xsjima/component-library#readme",
  "main": "dist/component-library.ssr.js",
  "browser": "dist/component-library.esm.js",
  "module": "dist/component-library.esm.js",
  "unpkg": "dist/component-library.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production tailwindcss -i ./src/main.css -o ./dist/output.css && rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prepare": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.26",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.45.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
